{
  "name": "mysql-plan-visualizer",
  "version": "1.19.0",
  "description": "A modern Vue.js component for visualizing and analyzing MySQL execution plans with interactive diagrams, performance insights, and query optimization recommendations",
  "homepage": "https://ahmmedsamier.github.io/MySql-Plan-Visualizer/",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "mysql",
    "explain",
    "visualizer",
    "query",
    "optimization",
    "database",
    "performance",
    "plan",
    "mysql-explain",
    "vue"
  ],
  "main": "./dist/mysql-plan-visualizer.umd.js",
  "module": "./dist/mysql-plan-visualizer.es.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/mysql-plan-visualizer.es.js",
      "require": "./dist/mysql-plan-visualizer.umd.js"
    },
    "./dist/mysql-plan-visualizer.css": "./dist/mysql-plan-visualizer.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:app": "vite build",
    "build:lib": "vue-tsc --noEmit && LIB=true vite build",
    "bun:build": "bun x vue-tsc --noEmit && bun x vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "prepare": "husky",
    "test": "vitest --run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@vueuse/core": "^14.1.0",
    "bootstrap": "^5.3.8",
    "d3": "^7.9.0",
    "d3-flextree": "^2.1.2",
    "highlight.js": "^11.11.1",
    "lodash": "^4.17.21",
    "motion-v": "^1.7.4",
    "splitpanes": "^3.2.0",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.25",
    "vue-clipboard3": "^2.0.0",
    "vue-tippy": "^6.7.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^24.7.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "sass": "^1.58.0",
    "typescript": "^5.7.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-singlefile": "^2.1.0",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
